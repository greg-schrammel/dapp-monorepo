{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "web#build": {
      "dependsOn": [
        "^build",
        "$NEXT_PUBLIC_INFURA_ID",
        "$NEXT_PUBLIC_ALCHEMY_ID",
        "$NEXT_PUBLIC_ETHERSCAN_API_KEY",
        "$NODE_ENV"
      ],
      "outputs": [".next/**"]
    },
    "docs#build": {
      "dependsOn": [
        "^build",
        "$links",
        "$VERCEL_GIT_REPO_OWNER",
        "$VERCEL_GIT_REPO_SLUG",
        "$VERCEL_GIT_COMMIT_REF"
      ],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
